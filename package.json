{
  "name": "entitry",
  "version": "0.0.1",
  "bin": {
    "entitry": "./dist/index.js"
  },
  "description": "A CLI tool to generate Prisma models interactively, inspired by Symfony's make:entity",
  "main": "index.js",
  "scripts": {
    "build": "tsup src/index.ts --format cjs --clean",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "prisma",
    "cli",
    "generator",
    "schema",
    "interactive",
    "database",
    "modeling"
  ],
  "author": "Nathan Piveteau piveteaunat@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nat-pvt/entitry.git"
  },
  "bugs": {
    "url": "https://github.com/nat-pvt/entitry/issues"
  },
  "homepage": "https://github.com/ton-pseudo/entitry#readme",
  "type": "commonjs",
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@mrleebo/prisma-ast": "^0.13.1",
    "cac": "^6.7.14",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
